    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.33.0",
    "npm-run-all2": "^6.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
